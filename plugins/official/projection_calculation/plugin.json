{
    "name": "projection_plugin",
    "version": "1.0.0",
    "description": "Plugin pour convertir des coordonnées 'Degrés, Minutes décimales' en d'autres formats (ex: WGS84 décimal, UTM, etc.).",
    "author": "MysterAI",
    "plugin_type": "python",
    "entry_point": "main.py",
    "categories": ["CoordinatesCalculations"],
    "dependencies": ["pyproj"],
    "input_types": {
      "coordinate_str": "string",
      "target_projection": "string"
    },
    "output_types": {
      "latitude_deg": "float",
      "longitude_deg": "float",
      "projected_x": "float",
      "projected_y": "float"
    }
  }
  