{
    "name": "metadetection",
    "version": "1.1.2",
    "description": "Plugin de détection de meta-informations sur une cache",
    "author": "MysterAI",
    "plugin_type": "python",
    "entry_point": "main.py",
    "dependencies": [],
    "categories": ["AlphabetsDecryption", "Decryption"],
    "input_types": {
        "text": {
            "type": "string",
            "label": "Texte à traiter",
            "placeholder": "Entrez le texte ici..."
        },
        "mode": {
            "type": "select",
            "label": "Mode",
            "options": ["decode", "detect"],
            "default": "decode"
        },
        "strict": {
            "type": "select",
            "label": "Mode strict",
            "options": ["strict", "smooth"],
            "default": "smooth"
        },
        "enable_gps_detection": {
            "type": "boolean",
            "label": "Détecter les coordonnées GPS",
            "default": true
        }
    },
    "output_types": {
        "encoded_text": {
            "type": "string",
            "label": "Texte chiffré"
        },
        "decoded_text": {
            "type": "string",
            "label": "Texte déchiffré"
        }
    }
}